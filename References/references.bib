issn = {00295493},
journal = {Nuclear Engineering and Design},
keywords = {Flow-induce vibration,Gas-liquid flow,Pipe flow,Two-phase flow},
month = {mar},
pages = {214--224},
publisher = {Elsevier B. V.},
title = {{Vibration response of a pipe subjected to two-phase flow: Analytical formulations and experiments}},
url = {http://dx.doi.org/10.1016/j.nucengdes.2016.12.020 https://linkinghub.elsevier.com/retrieve/pii/S0029549316305222},
volume = {313},
