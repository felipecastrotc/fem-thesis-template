@article{Xia2010,
abstract = {In this paper an improved model is developed aimed at analyzing the fluidelastic vibration of a single flexible cylinder surrounded by rigid cylinders and subject to cross-flow. Compared with other dynamical models reported previously, the nonlinearity associated with the mean axial extension of the cylinder has been accounted for in the current work. Calculations are performed to explore the effect on the nonlinear dynamics of the nonlinearity presented in the equation of motion. Numerical results show that, with increasing flow velocity just beyond a critical value, a post-Hopf limit-cycle motion occurs. For a system without loose supports, the vibration amplitude of the limit-cycle motion can be predicted by using the presented model. To understand the nonlinear dynamics of the cylinder better, a modified system with loose supports is also investigated. The impacting force induced by the loose supports was modelled by a cubic spring or by a trilinear spring. It is found, by using the theoretical model developed here, that the post-Hopf dynamics predicted with the cubic-spring representation is quite different from those predicted with the trilinear-spring representation. However, it is shown that the effect of the nonlinearity associated with the mean axial extension on the nonlinear dynamics of cylinder with loose supports is not pronounced. {\textcopyright} 2010 Elsevier B. V. All rights reserved.},
author = {Xia, W. and Wang, L.},
doi = {10.1016/j.nucengdes.2010.03.024},
file = {:home/fctc/Library/Mendeley/Xia, Wang/Nuclear Engineering and Design/Xia, Wang - 2010 - The effect of axial extension on the fluidelastic vibration of an array of cylinders in cross-flow.pdf:pdf},
isbn = {00295493},
issn = {00295493},
journal = {Nuclear Engineering and Design},
number = {7},
pages = {1707--1713},
publisher = {Elsevier B. V.},
title = {{The effect of axial extension on the fluidelastic vibration of an array of cylinders in cross-flow}},
url = {http://dx.doi.org/10.1016/j.nucengdes.2010.03.024},
volume = {240},
year = {2010}
}
@incollection{tagkey20143,
abstract = {In this chapter, straight tubes, tube bundles, flexible plates, and shells subjected to external parallel flow are presented. Leakage-flow-induced vibrations are also discussed. In the case of straight tubes and tube bundles, most cases of concern involve random vibrations. Evaluation methods for vibration amplitude in single-phase flow as well as two-phase flow are first discussed. Flutter and divergence instabilities which are expected to occur at high flow velocities are discussed next. Structural stiffness is relatively low for flexible plates and shells. These structures are therefore more susceptible to instabilities even at low flow velocities. The feedback force-related structural vibration can be relatively large for structures subjected to leakage-flow. In this case, divergence and flutter can easily occur at relatively low flow velocity. The associated flutter and divergence analysis is presented here.},
