keywords = {Bladeless wind turbine,Dynamic modeling,Lock-in phenomenon,Vortex induced vibration},
pages = {632--643},
publisher = {Elsevier Ltd},
title = {{Dynamic modeling of vortex induced vibration wind turbines}},
url = {https://doi.org/10.1016/j.renene.2018.01.038},
volume = {121},
year = {2018}
