file = {:home/fctc/Library/Mendeley/Sim, Bae, Mureithi/Journal of Mechanical Science and Technology/Sim, Bae, Mureithi - 2010 - An experimental study on characteristics of two-phase flows in vertical pipe.pdf:pdf},
