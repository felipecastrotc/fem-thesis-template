doi = {10.1115/1.2388994},
file = {:home/fctc/Library/Mendeley/Riverin, Pettigrew/Journal of Pressure Vessel Technology/Riverin, Pettigrew - 2007 - Vibration Excitation Forces Due to Two-Phase Flow in Piping Elements.pdf:pdf},
