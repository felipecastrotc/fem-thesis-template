pages = {875--881},
title = {{Characteristics of slug flow in a vertical riser}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-78650486874{\&}partnerID=40{\&}md5=9fc411cff503a4075dcb8022ed14333d},
volume = {2},
year = {2010}
}
